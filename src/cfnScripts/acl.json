{
	"AWSTemplateFormatVersion":"2010-09-09",
	"Description":"test template for cnfassist - creates a single acl",
	"Parameters":{
		"env":{
			"Type":"String"
		},
		"subnet":{
			"Type":"String",
			"Description":"::testSubnet"
		}
		
	},
	"Mappings":{
		"environMap":{
			"Test":{
				"vpc":"vpc-38d62752"
			}		
		}
	},
	"Resources":{
		"assocAclWebSubnet":{
			"Type":"AWS::EC2::SubnetNetworkAclAssociation",
			"Properties":{
				"SubnetId":{
					"Ref":"subnet"
				},
				"NetworkAclId":{
					"Ref":"testAcl"
				}			
			}
		},
		"testAcl":{
			"Type":"AWS::EC2::NetworkAcl",
			"Properties":{
				"Tags":[
					{
						"Key":"Name",
						"Value":"testAcl"
					}
					
				],
				"VpcId":{
					"Fn::FindInMap":[
						"environMap",
						{
							"Ref":"env"
						},
						"vpc"
					]
					
				}
				
			}
			
		},
		"networkAclEntryWebE100":{
			"Type":"AWS::EC2::NetworkAclEntry",
			"Properties":{
				"NetworkAclId":{
					"Ref":"testAcl"
				},
				"RuleNumber":"100",
				"RuleAction":"allow",
				"Egress":"true",
				"CidrBlock":"0.0.0.0/0",
				"Protocol":"6",
				"PortRange":{
					"From":"1024",
					"To":"65535"
				}
				
			}
			
		}
		
	}
	
}
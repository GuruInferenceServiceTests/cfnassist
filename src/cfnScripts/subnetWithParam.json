{
	"AWSTemplateFormatVersion": "2010-09-09",
	"Description" : "test template for cnfassist - creates a single subnet",
	"Parameters" : {
    	"env" : { "Type" : "String" },
    	"zoneA" : { "Type" : "String" , "Description" : "zoneADescription" },
    	"zoneB" : { "Type" : "String" , "Default" : "eu-west-1b" }
  	},
  	"Mappings" : {
      "environMap" : {
        "Test" : { "vpc" : "vpc-38d62752" }
      }
    },
	"Resources" : {
		"testSubnet" : {
			"Type" : "AWS::EC2::Subnet",
			"Properties" : {
      			"AvailabilityZone" : { "Ref" : "zoneA" },
	      		"CidrBlock" : "10.0.10.0/24",
      			"Tags" : [ { "Key" : "Name", "Value": "webSubnetZoneA" } ],
      			"VpcId" : { "Fn::FindInMap" : [ "environMap", { "Ref" : "env" }, "vpc" ]}
   			}
    	}
	}
}
